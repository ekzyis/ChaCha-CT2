<ResourceDictionary x:Class="Cryptool.Plugins.ChaChaVisualizationV2.Styles.Button"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="PrevButton" TargetType="{x:Type Button}">
        <Setter Property="Content" Value="&#x1f860;" />
        <Setter Property="Width" Value="25"/>
        <Setter Property="Height" Value="25"/>
    </Style>
    <Style x:Key="NextButton" TargetType="{x:Type Button}">
        <Setter Property="Content" Value="&#x1f862;" />
        <Setter Property="Width" Value="25"/>
        <Setter Property="Height" Value="25"/>
    </Style>
</ResourceDictionary>